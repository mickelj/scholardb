doctype html
html
  head
    title= title
    include jquery.pug
    include materialize.pug
    link(rel='stylesheet', href='/css/style.css')
  body
    header.row
      div.col.s8 
        h1 #{message}
      div.col.s4
        p.right-align
          small
            em Presented by the Washington and Lee University Library
    
    main.row
      block content
      
    footer.row
      div.col.s12
        p.center-align
          small &copy; 2017, Washington and Lee University Library
